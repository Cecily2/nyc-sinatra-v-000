<h1>Edit Landmark</h1>

<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" name="_method" value="PATCH">

  Landmark Name: <input type="text" id="name" name="landmark[name]" value="<%= @landmark.name %>"><br><br>
  Year Completed: <input type="text" id="year_completed" name="landmark[year_completed]" value="<%= @landmark.year_completed %>"><br><br>

 <input type="submit" name="Edit Landmark" value="Edit Landmark">

</form>